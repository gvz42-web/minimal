{
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/.angular": true,
    "**/.git": true,
    "**/.idea": true,
    "**/.vscode": true,
    "**/out": true,
    "**/coverage": true,
    "**/.cache": true,
    "**/tmp": true,
    "**/logs": true,
    "**/package-lock.json": true
  },

  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/.angular/**": true,
    "**/coverage/**": true,
    "**/.git/**": true
  },
  "i18n-ally.localesPaths": ["src/assets/i18n", "src/app/shared/core/i18n"],
  "i18n-ally.keystyle": "nested"
}
